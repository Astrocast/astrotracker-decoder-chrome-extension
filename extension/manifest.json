{
  "name": "AstroTrackerDecoder",
  "description": "AstroTracker payload decoder",
  "version": "0.2",
  "manifest_version": 3,
  "icons": { 
    "16": "icons/astrocast_icon_16.png",
    "48": "icons/astrocast_icon_48.png",
    "128": "icons/astrocast_icon_128.png"
  },
  "content_scripts": [{
    "matches": ["https://portal.astrocast.com/Message/Details/*"],
    "js": ["decoder.js"]
  }]
}
